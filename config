# Default compiler
CC=gcc
# Default linker
LD=ld
# Default libtool
LIBTOOL=libtool
# AR
AR=AR

# Make directory
MKDIR=mkdir

# HOST
OS=XMMS_OS_LINUX

# Flags
DEBUG=-g -O0
CFLAGS=-Wall -Werror $(DEBUG)

# DEPENDS
DBUS_CFLAGS=-I/usr/include/dbus-1.0 -I/usr/lib/dbus-1.0/include
DBUS_LINKFLAGS=-ldbus-1 -ldbus-glib-1
GLIB_CFLAGS=-I/usr/include/glib-2.0 -I/usr/lib/glib-2.0/include
GLIB_LINKFLAGS=-lglib-2.0 -lgmodule-2.0 -lgthread-2.0
SQLITE_CFLAGS=
SQLITE_LINKFLAGS=-lsqlite
MATH_LINKFLAGS=-lm
MAD_LINKFLAGS=-lmad -lm

# DIRS
PREFIX=/usr/local
OUTPUT_PRG=builddir/bin
